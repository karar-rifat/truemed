<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
                                                   xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
                                                   android:layout_height="match_parent">

    <EditText
            android:layout_width="300dp"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:hint="Table Name"
            android:ems="10"
            android:id="@+id/editText47" android:layout_marginStart="8dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginEnd="8dp"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintHorizontal_bias="0.497"
            android:layout_marginTop="32dp" app:layout_constraintTop_toBottomOf="@+id/textView61"
            android:layout_marginBottom="8dp" app:layout_constraintBottom_toBottomOf="parent"/>
    <TextView
            android:text="Add New Table"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView61" android:layout_marginTop="8dp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="32dp"
            android:textSize="24sp" app:layout_constraintStart_toEndOf="@+id/imageView32"
            android:layout_marginStart="8dp"/>
    <ImageView
            android:layout_width="30dp"
            android:layout_height="30dp" app:srcCompat="@android:drawable/btn_dialog"
            android:id="@+id/imageView32"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="8dp"
            android:layout_marginTop="8dp" app:layout_constraintTop_toTopOf="parent"/>
    <Button
            android:text="Create"
            android:layout_width="300dp"
            android:layout_height="wrap_content"
            android:id="@+id/button25"
            app:layout_constraintTop_toBottomOf="@+id/editText47" app:layout_constraintStart_toStartOf="@+id/editText47"
            app:layout_constraintEnd_toEndOf="@+id/editText47"/>
</androidx.constraintlayout.widget.ConstraintLayout>